CREATE TABLE Empresa_Parceira 
( 
    Email VARCHAR(100),  
    id_Empresa_parceira SERIAL PRIMARY KEY,  
    Telefone VARCHAR(20),  
    CNPJ VARCHAR(20) 
); 

CREATE TABLE Entity 
( 
    -- Adicione as colunas necess√°rias
    id_Entity SERIAL PRIMARY KEY,
    nome VARCHAR(100),
    endereco VARCHAR(255)
); 

CREATE TABLE funcionario 
( 
    id_funcionario SERIAL PRIMARY KEY,  
    telefone VARCHAR(20),  
    cpf VARCHAR(20),  
    nome VARCHAR(100),  
    idEmpresa_Parceira INT REFERENCES Empresa_Parceira (id_Empresa_parceira)
); 

CREATE TABLE Tecnologia 
( 
    id_Tecnologia SERIAL PRIMARY KEY,  
    nome VARCHAR(100),  
    area VARCHAR(100),  
    descricao TEXT 
); 

CREATE TABLE Utiliza 
( 
    id_Utiliza SERIAL PRIMARY KEY,  
    id_Tecnologia INT REFERENCES Tecnologia (id_Tecnologia),
    id_funcionario INT REFERENCES funcionario (id_funcionario)
); 

CREATE TABLE Fornece 
( 
    id_Fornece SERIAL PRIMARY KEY,  
    id_Tecnologia INT REFERENCES Tecnologia (id_Tecnologia),
    id_Empresa_parceira INT REFERENCES Empresa_Parceira (id_Empresa_parceira)
); 
